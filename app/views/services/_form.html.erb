<div class="row">
  <div class="col-12 col-lg-6">
    <%= simple_form_for @service do |f| %>
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :active %>
      <%= f.input :slug %>
      <%= f.input :images %>
      <%= f.association :categories, prompt: t('prompt.categories')%>
      <%= f.submit class: "btn btn-primary" %>
    <% end %>
  </div>
  <div class="col-12 col-lg-6">
    <h5>Crear Items</h5>
  </div>
</div>
