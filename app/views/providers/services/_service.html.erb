<div class="col col-sm-6 col-md-4">
  <div class="card service-card dashboard-service-card mb-4">
    <div class="card-image">
      <%= image_tag "https://source.unsplash.com/random/?avatar", class: "card-img-top" %>
    </div>
    <div class="card-body">
      <p class="card-text card-description"><%= service.name %></p>
    </div>
    <div class="card-footer d-flex justify-content-end">
      <span class="card-price"><%= service.price %></span>
    </div>
  </div>
</div>
